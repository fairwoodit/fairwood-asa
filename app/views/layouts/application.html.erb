<!DOCTYPE html>
<html>
<head>
  <title>FairwoodAsa</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if parent_signed_in? %>
  Signed in as <%= current_parent.email %>. Not you?
  <%= link_to "Edit profile", edit_parent_registration_path %>
  <%= link_to "Sign out", destroy_parent_session_path, :method => :delete %>
<% else %>
  <%= link_to "Sign up", new_parent_registration_path %> or <%= link_to "sign in", new_parent_session_path %>
<% end %>

<!--
<p class="notice"><\%= notice %></p>
<p class="alert"><\%= alert %></p>
-->

<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, id: "flash_#{name}" %>
<% end %>

<%= yield %>

</body>
</html>
